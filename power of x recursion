import java.util.*;
import java.lang.*;
import java.io.*;

class Main {
        int count(int x,int n){
            if(n==0){
                return 1;
            }
            return x*count(x,n-1);
        }
   public static void main(String[] args){
       Main m=new Main();
       int res=m.count(2,3);
       System.out.println(res);
   }
}
